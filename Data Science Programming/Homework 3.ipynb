    "#Regression set up\n",
    "import statsmodels.api as sm\n",
    "from patsy import dmatrices\n",
    "\n",
    "#Create a new DF with just count and mean\n",
    "rater_goodness_querry=rater_goodness[[\"count\",\"mean\"]]\n",
    "\n",
    "#Create design matricies; we want to fit Mean using Count\n",
    "y, X = dmatrices(\"mean ~ count\", data=rater_goodness_querry, return_type='dataframe')\n",
    "\n",
    "#Run linear regression model\n",
    "model = sm.OLS(y, X) # Set up the model\n",
    "result = model.fit() # Fit model (find the intercept and slopes)\n",
    "print(result.summary())"
   ]
